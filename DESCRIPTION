Package: rtide
Title: Tide Heights
Version: 0.0.10
Authors@R: c(
    person("Joe", "Thorley", , "joe@poissonconsulting.ca", role = "aut",
           comment = c(ORCID = "0000-0002-7683-4592")),
    person("Luke", "Miller", , "contact@lukemiller.org", role = c("aut", "cre")),
